{
  "name": "sta-node",
  "version": "0.1.0",
  "description": "3M STA server",
  "main": "index.js",
  "scripts": {
    "test": "node ./node_modules/jest/bin/jest.js --runInBand --config=./jest.config.js",
    "start:local": "APP_ENV=local nodemon src/runtime/index.js -e js,graphql",
    "start:develop": "APP_ENV=develop nodemon src/runtime/index.js -e js,graphql",
    "start:staging": "APP_ENV=staging src/runtime/node index.js",
    "start": "APP_ENV=production node src/runtime/index.js",
    "lint": "eslint ./",
    "dev": "node ./node_modules/webpack/bin/webpack.js --config webpack.local.js",
    "precommit": "npm run-script lint && npm run-script test"
  },
  "author": "AIM Consulting",
  "license": "UNLICENSED",
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "7.0.1",
    "@graphql-tools/load": "7.0.1",
    "@graphql-tools/load-files": "6.3.2",
    "apollo-server": "3.0.2",
    "apollo-server-express": "3.0.2",
    "axios": "0.21.1",
    "cors": "2.8.5",
    "dotenv": "^10.0.0",
    "express": "4.17.1",
    "graphql": "15.5.0",
    "jsonwebtoken": "8.5.1",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "eslint": "7.31.0",
    "jest": "27.0.6",
    "nodemon": "2.0.12"
  },
  "_moduleAliases": {
    "@root": ".",
    "@services": "src/services",
    "@models": "src/models"
  }
}
